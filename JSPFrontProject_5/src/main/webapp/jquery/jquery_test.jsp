<script type="text/javascript">
$(function(){
	let position=parseInt($('#qmenu').css('top')) // 50px
	//alert(position)
	$(window).scroll(function(){
		let pos=$(window).scrollTop();
		$('#qmenu').stop().animate({"top":pos+position+"px"},1000)//시간 (수행시간 5초후)
		// Thread.sleep()  delay  
	})
	
	$('#sendMsg').keydown(function(key){
		if(key.keyCode===13) // enter
		{
			
			let msg=$('#sendMsg').val()
			if(msg.trim()==="")
			{
				$('#sendMsg').focus()
				return
			}
			$('#recvMsg').append(msg+"<br>")
			
			$('#sendMsg').val("")
			$('#sendMsg').focus()	
			
			let ch=$('#chatArea').height()
	        let m=$("#recvMsg").height()-ch
	        $('#chatArea').scrollTop(m)
		}
	})
})
</script>
